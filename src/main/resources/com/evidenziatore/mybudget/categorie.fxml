<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.evidenziatore.mybudget.controller.ControllerCategorie">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>

    <TableView fx:id="tableViewCategorie" prefWidth="600">
        <columns>
            <TableColumn fx:id="colId" text="ID" prefWidth="100" />
            <TableColumn fx:id="colValore" text="Valore" prefWidth="300" />
            <TableColumn fx:id="colImportanza" text="Importanza" prefWidth="200" />
        </columns>
    </TableView>
</VBox>
